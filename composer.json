{
  "name": "gpslab/pagination-bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "Pagination bundle",
  "keywords": ["php", "pagination", "symfony", "doctrine"],
  "homepage": "http://github.com/gpslab/pagination-bundle",
  "autoload": {
    "psr-4": {
      "GpsLab\\Bundle\\PaginationBundle\\": "src/"
